RUSTFLAGS="-C target-cpu=native -C opt-level=2"
COMMON_FLAGS="-march=native -O3 -pipe -flto=12 -fno-semantic-interposition -pipe -fno-stack-protector -fgraphite-identity -floop-nest-optimize"
